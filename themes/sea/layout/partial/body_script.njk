{{ js('js/mobile_nav') }}

{% if theme.algolia %}
<script src="https://cdn.jsdelivr.net/npm/@docsearch/js@3"></script>
<script type="text/javascript">
    docsearch({
        container: '#sea-search-input',
        appId: '{{ theme.algolia.appId }}',
        apiKey: '{{ theme.algolia.apiKey }}',
        indexName: '{{ theme.algolia.indexName }}',
    });
</script>
{% endif %}